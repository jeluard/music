{
    "compilerOptions": {
      "target": "es2015",
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "baseUrl": ".",
      "allowJs": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true,
      "jsx": "react",
    },
    "include": [
      "src"
    ], "exclude": [
      "node_modules"
   ]
  }
